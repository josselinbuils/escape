{
  "name": "escape",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-client": "react-scripts build",
    "lint": "yarn lint-client && yarn lint-server",
    "lint-client": "tslint -c src/tslint.json -p tsconfig.json src/**/*.tsx",
    "lint-server": "tslint -c server/tslint.json -p server/tsconfig.json server/**/*.ts",
    "start": "yarn build-client && yarn start-server",
    "start-client": "react-scripts start",
    "start-server": "ts-node --project server/tsconfig.json server/server.ts",
    "test-client": "react-scripts test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "bootstrap": "^4.1.3",
    "express": "^4.16.4",
    "express-content-length-validator": "^1.0.0",
    "fs-extra": "^7.0.1",
    "helmet": "^3.14.0",
    "node-sass": "^4.10.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-scripts": "^2.1.1",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "serve-static": "^1.13.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/reactstrap": "^6.4.2",
    "@types/ws": "^6.0.1"
  }
}
